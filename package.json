{
  "name": "nuxt-surrealdb",
  "version": "0.0.6",
  "description": "A Nuxt module aimed to simplify the use of SurrealDB",
  "keywords": [
    "nuxt",
    "nuxt-module",
    "surrealdb"
  ],
  "repository": "https://github.com/sandros94/nuxt-surrealdb",
  "license": "MIT",
  "type": "module",
  "author": {
    "name": "Sandro Circi",
    "email": "sandro.circi@digitoolmedia.com"
  },
  "build": {
    "externals": [
      "defu",
      "ofetch",
      "ufo"
    ]
  },
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "release": "nr lint && nr test && nr prepack && changelogen --minor --release && na publish && git push --follow-tags",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:types": "vue-tsc --noEmit && cd playground && vue-tsc --noEmit"
  },
  "dependencies": {
    "@nuxt/kit": "^3.11.2",
    "@vueuse/core": "^10.10.0",
    "undio": "^0.2.0"
  },
  "devDependencies": {
    "@nuxt/devtools": "^1.2.0",
    "@nuxt/eslint-config": "^0.3.10",
    "@nuxt/module-builder": "^0.6.0",
    "@nuxt/schema": "^3.11.2",
    "@nuxt/test-utils": "^3.12.1",
    "@types/node": "^20.12.11",
    "changelogen": "^0.5.5",
    "eslint": "^9.2.0",
    "nuxt": "^3.11.2",
    "typescript": "latest",
    "vitest": "^1.6.0",
    "vue-tsc": "^2.0.16"
  }
}
